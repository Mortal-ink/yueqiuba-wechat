.titleImage {
    width: 100%;
}

.cu-form-group .title {
    min-width: calc(4em + 30rpx);
}

.notHaveList-img {
    width: 250rpx;
    margin-top: 50rpx;
    margin-bottom: 50rpx;
    height: 250rpx;
    border-radius: 200rpx;
    background: #fff;
    box-shadow: 0 0 8px rgba(0,0,0,0.14),0 8px 16px rgba(0,0,0,0.28);
}

.OuterFrame {
    width: 94%;
    height: 85%;
    margin: 0 3%;
    margin-top: 20px;
    margin-bottom: 20px;
    border-radius: 15rpx;
    box-shadow: 0 0 4px rgba(0,0,0,0.14),0 4px 8px rgba(0,0,0,0.28);
    overflow: scroll;
    background-color: #ecedf3;
}

.remind-box {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.font-l2-color {
    color: #fff;
}

.margin-top-300 {
    margin-top: 300rpx;
}

.font-size-30 {
    font-size: 30rpx;
}

.padding-l-20 {
    padding-left: 20rpx;
}

.remind-img {
    width: 250rpx;
    height: 250rpx;
    padding-bottom: 25rpx;
}

.swiper {
    height: 400rpx;
    width: 100%;
}

.swiper image {
    height: 100%;
    width: 100%;
}

page {
    margin: 0;
    font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol";
    font-size: 24rpx;
    font-weight: 400;
    line-height: 1.42;
    color: #4a4a4a;
    text-align: left;
}

.container {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.article-list-item {
    position: relative;
    margin-bottom: 18rpx;
    height: 174rpx;
    background-color: #ccc;
    text-align: center;
    overflow: hidden;
}

.article-list-item text {
    position: relative;
    font-size: 40rpx;
    color: #fff;
    line-height: 174rpx;
    z-index: 1;
}

.article-list-item image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
}

.list-text {
    float: left;
}

.hero_name {
    font-size: 32rpx;
    color: #474747;
}

.new_tag {
    position: absolute;
    top: 62rpx;
    display: inline-block;
    min-width: 38px;
    height: 19px;
    background: linear-gradient(to right,#FF9966,#FF5E62);
    font-size: 28rpx;
    color: #fff;
    line-height: 18px;
    border-radius: 10px 10px 10px 2px;
    text-align: center;
    margin-left: 6px;
}

.list-icon {
    float: right;
    margin-top: 20rpx;
    margin-right: 28rpx;
    width: 60rpx;
    height: 60rpx;
}

.list-icon image {
    width: 100%;
    height: 100%;
    color: #D2D6DE;
}

.list {
    margin-top: 50rpx;
}

.list-item {
    position: relative;
    margin-bottom: 24rpx;
    height: 160rpx;
    background-color: #fff;
    border-radius: 4px 40px 40px 4px;
    box-shadow: 0px 1px 4px #D8DDE5;
    line-height: 160rpx;
    overflow: hidden;
}

.list-avatar {
    position: relative;
    margin: 20rpx 16rpx 0 20rpx;
    float: left;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    overflow: hidden;
}

.list-img {
    position: absolute;
    top: -3%;
    left: -3%;
    width: 106%;
    height: 106%;
}
.picker{
    height: 100%;
}