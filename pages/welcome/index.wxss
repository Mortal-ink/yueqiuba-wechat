h1,h2 {
    display: block;
}

time,span,s {
    display: inline-block;
    vertical-align: top;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.small-btn {
    position: absolute;
    border-radius: 4px;
    margin-top: -27rpx;
    top: 50%;
    right: 30rpx;
    width: 114rpx;
    line-height: 54rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    background: #fe6500;
}

.small-btn.active {
    color: #999;
    background: #f2f2f2;
}

.layer-phone {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    background: rgba(0,0,0,0.38);
    z-index: 10;
}

.layer-phone-box {
    position: absolute;
    border-radius: 6px;
    top: 40%;
    left: 50%;
    width: 490rpx;
    transform: translate(-50%,-50%);
    overflow: hidden;
    background: #fff;
}

.layer-phone text {
    display: block;
    line-height: 100rpx;
    font-size: 30rpx;
}

.layer-phone .btn-layer {
    display: flex;
    border-top: 1px solid #e6e6e6;
    overflow: hidden;
}

.layer-phone .btn-layer view {
    float: left;
    flex: 1;
    border-right: 1px solid #e6e6e6;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #2c69e2;
}

.layer-phone .btn-layer view:last-of-type {
    border-right: none;
}

.bg {
    position: absolute;
    width: 100%;
    height: 752rpx;
    z-index: 0;
}

page {
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    font-size: 10pt;
    line-height: 150%;
    min-height: 100%;
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: stretch;
}

.container {
    position: relative;
    flex: 1;
    display: flex;
    flex-direction: column;
    background: #fe6500;
    align-items: stretch;
    padding: 0;
    height: 100%;
    overflow: hidden;
    justify-content: space-between;
    box-sizing: border-box;
}

.content {
    flex: 1;
    display: flex;
    position: relative;
    z-index: 10;
    flex-direction: column;
    align-items: stretch;
    justify-content: center;
    width: 100%;
    height: 100%;
    padding-bottom: 450rpx;
    background: -webkit-gradient(linear,left top,left bottom,from(rgba(244,244,244,0)),color-stop(0.1,#f4f4f4),to(#f4f4f4));
    opacity: 0;
    transform: translate3d(0,100%,0);
    animation: rise 2.5s cubic-bezier(0.19,1,0.22,1) 0s forwards;
}

.remind-box {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 300rpx;
}

.remind-img {
    width: 250rpx;
    height: 250rpx;
    padding-bottom: 25rpx;
}

.login-img {
    width: 250rpx;
    height: 250rpx;
    position: absolute;
    left: 0;
    right: 0;
    margin-top: 150rpx;
    border-radius: 100rpx;
    margin-left: auto;
    margin-right: auto;
}

@-webkit-keyframes rise {
    0% {
        opacity: 0;
        transform: translate3d(0,100%,0);
    }

    50% {
        opacity: 1;
    }

    100% {
        opacity: 1;
        transform: translate3d(0,58%,0);
    }
}

@keyframes rise {
    0% {
        opacity: 0;
        transform: translate3d(0,100%,0);
    }

    50% {
        opacity: 1;
    }

    100% {
        opacity: 1;
        transform: translate3d(0,58%,0);
    }
}

@-webkit-keyframes show {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 0.95;
    }
}

@keyframes show {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 0.95;
    }
}

.hd {
    position: absolute;
    top: 0;
    left: 50%;
    width: 1000rpx;
    margin-left: -500rpx;
    height: 220rpx;
    transition: all 0.35s ease;
}

.logo {
    position: absolute;
    z-index: 2;
    left: 50%;
    bottom: 200rpx;
    width: 400rpx;
    height: 400rpx;
    margin-left: -200rpx;
    animation: sway 10s ease-in-out infinite;
    background: #fff;
    border-radius: 50%;
}

@-webkit-keyframes sway {
    0% {
        transform: translate3d(0,20rpx,0) rotate(-15deg);
    }

    17% {
        transform: translate3d(0,0rpx,0) rotate(25deg);
    }

    34% {
        transform: translate3d(0,-20rpx,0) rotate(-20deg);
    }

    50% {
        transform: translate3d(0,-10rpx,0) rotate(15deg);
    }

    67% {
        transform: translate3d(0,10rpx,0) rotate(-25deg);
    }

    84% {
        transform: translate3d(0,15rpx,0) rotate(15deg);
    }

    100% {
        transform: translate3d(0,20rpx,0) rotate(-15deg);
    }
}

@keyframes sway {
    0% {
        transform: translate3d(0,20rpx,0) rotate(-15deg);
    }

    17% {
        transform: translate3d(0,0rpx,0) rotate(25deg);
    }

    34% {
        transform: translate3d(0,-20rpx,0) rotate(-20deg);
    }

    50% {
        transform: translate3d(0,-10rpx,0) rotate(15deg);
    }

    67% {
        transform: translate3d(0,10rpx,0) rotate(-25deg);
    }

    84% {
        transform: translate3d(0,15rpx,0) rotate(15deg);
    }

    100% {
        transform: translate3d(0,20rpx,0) rotate(-15deg);
    }
}

.wave {
    max-width: none;
    position: absolute;
    z-index: 3;
    right: 0;
    bottom: 0;
    opacity: 0.725;
    height: 260rpx;
    width: 2250rpx;
    animation: wave 10s linear infinite;
}

.wave-bg {
    z-index: 1;
    animation: wave-bg 10.25s linear infinite;
}

@-webkit-keyframes wave {
    from {
        transform: translate3d(125rpx,0,0);
    }

    to {
        transform: translate3d(1125rpx,0,0);
    }
}

@keyframes wave {
    from {
        transform: translate3d(125rpx,0,0);
    }

    to {
        transform: translate3d(1125rpx,0,0);
    }
}

@-webkit-keyframes wave-bg {
    from {
        transform: translate3d(375rpx,0,0);
    }

    to {
        transform: translate3d(1375rpx,0,0);
    }
}

@keyframes wave-bg {
    from {
        transform: translate3d(375rpx,0,0);
    }

    to {
        transform: translate3d(1375rpx,0,0);
    }
}

.bd {
    position: relative;
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: stretch;
    animation: bd-rise 2s cubic-bezier(0.23,1,0.32,1) 2s forwards;
    opacity: 0;
}

@-webkit-keyframes bd-rise {
    from {
        opacity: 0;
        transform: translate3d(0,60rpx,0);
    }

    to {
        opacity: 1;
        transform: translate3d(0,0,0);
    }
}

@keyframes bd-rise {
    from {
        opacity: 0;
        transform: translate3d(0,60rpx,0);
    }

    to {
        opacity: 1;
        transform: translate3d(0,0,0);
    }
}

.confirm-btn {
    padding: 0;
    width: 400rpx;
    font-size: 40rpx;
    line-height: 80rpx;
    height: 80rpx;
    background: #fe6500;
    color: #fff;
    text-align: center;
    border-radius: 10rpx;
    margin: 150rpx auto 0;
}

.confirm-btn:active {
    opacity: 0.8;
}

.copyright {
    font-size: 24rpx;
    color: #999;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 30rpx;
    text-align: center;
    z-index: 10;
}