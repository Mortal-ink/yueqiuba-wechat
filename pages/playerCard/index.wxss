    font-size: 33rpx;
}

.scorePage {
    position: fixed;
    margin: 0rpx;
    width: 100%;
    height: 100%;
    bottom: 0rpx;
    background-color: rgba(194,189,189,0.3);
    z-index: 3;
}

.scoreView {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    flex-wrap: wrap;
}

.scoreContain {
    height: 370rpx;
    width: 100%;
    position: absolute;
    bottom: 0rpx;
    background-color: #f2f2f2;
}

.scoreButton {
    width: 110rpx;
    height: 110rpx;
    margin: 20rpx;
    border-radius: 55rpx;
    color: white;
    background-color: lightslategray;
    text-align: center;
    line-height: 110rpx;
}