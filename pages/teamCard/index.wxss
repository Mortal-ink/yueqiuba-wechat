  .cu-form-group {
    background-color: var(--white);
    padding: 1rpx 30rpx;
    display: flex;
    align-items: center;
    min-height: 100rpx;
    justify-content: space-between;
  }
  .page,.page__bd {
    margin-top: 60rpx;
    border-radius: 25rpx 25rpx 0 0;
    min-height: 100vh;
  }
  
  .userlogo {
    padding: 40rpx 0;
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  
  .userlogo-img {
    margin-bottom: 20rpx;
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
  }
  
  /* .panel {
    background: #FFFFFF;
  } */
  
  .login-img {
    width: 250rpx;
    height: 250rpx;
    position: absolute;
    left: 0;
    right: 0;
    margin-top: 250rpx;
    border-radius: 100rpx;
    margin-left: auto;
    margin-right: auto;
  }
  
  .list {
    margin-bottom: 150rpx;
  }
  
  .nick_name {
    font-size: 15px;
    padding-left: 20rpx;
    
    max-width: 200rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }
  
  .panel-title {
    /* display: flex; */
    margin-top: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .detail-navigator {
    height: 30rpx;
  }
  
  .title {
    font-family: PingFangSC-Regular;
    font-size: 40rpx;
    color: #F9B646;
    text-align: left;
    display: flex;
  }
  
  .teamtitle {
    font-family: PingFangSC-Regular;
    font-size: 40rpx;
    color: #4B0082;
    text-align: center;
    width: 100%;
  }
  
  .title-1 {
    font-family: PingFangSC-Regular;
    font-size: 35rpx;
    color: #4B0082;
    text-align: left;
  }
  
  .total {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #2A2A2A;
    text-align: center;
    padding-left: 10rpx;
    letter-spacing: 2rpx;
  }
  
  .block {
    border-left: 6rpx solid #F9B646;
    height: 35rpx;
    margin-right: 15px;
  }
  
  .line {
    border-top: 3px solid #F9B646;
    width: 20rpx;
    padding-bottom: 23rpx;
  }
  
  .role-logo {
    width: 88rpx;
    height: 33rpx;
    padding-left: 15rpx;
  }
  
  .img-logo {
    width: 250rpx;
    height: 250rpx;
    align-items: center;
    top: 20rpx;
    bottom: 20rpx;
    border-radius: 15rpx;
  }
  
  .panel-header {
    display: flex;
  }
  
  .header-text {
    margin-left: 90rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    height: 200rpx;
  }
  
  .thermal-value {
    display: flex;
    align-items: center;
  }
  
  .thermal-text {
    padding: 0 20rpx;
  }
  
  .squad-top-one-show-img {
    width: 172rpx;
    height: 48rpx;
    line-height: 48rpx;
    position: absolute;
    color: white;
    text-align: center;
    border-radius: 30rpx;
  }
  
  .name.name-top-one {
    line-height: 1;
    padding: 20rpx 0 0 0;
  }
  
  .squad-top-one-wrap {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #FFEFA6;
    display: flex;
    margin-left: 10rpx;
    align-items: center;
    justify-content: flex-start;
    line-height: 1;
  }
  
  .squad-top-one-text {
    border-radius: 0rpx 30rpx 30rpx 0rpx;
    background: #2A2A2A;
    padding: 8rpx 20rpx 8rpx 30rpx;
    margin-left: 152rpx;
    text-align: center;
    line-height: 33rpx;
  }
  
  .squad-heat-value,.squad-member-heat-value {
    font-family: HelveticaNeue-Bold;
    font-size: 44rpx;
    color: #E59D25;
    line-height: 1;
  }
  
  .name {
    text-align: center;
    font-family: PingFangSC-Medium;
    font-size: 50rpx;
    color: #2A2A2A;
    line-height: 67rpx;
    padding: 30rpx 0;
  }
  
  .detail-img {
    width: 30rpx;
    height: 30rpx;
  }
  
  .squad-card {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    border-radius: 5%;
    padding: 10rpx 20rpx 10rpx 0;
    background-color: #BDB76B;
  }
  
  .squad-card {
    border-bottom: 1rpx solid #D8D8D8;
  }
  
  .squad-card:last-child {
    border-bottom: none;
  }
  
  .squad-card:not(:last-child) {
    margin-bottom: 20rpx;
  }
  
  .squad-card-user {
    flex: 1;
    display: flex;
    margin-left: 15rpx;
  }
  
  .squad-card-distance {
    min-width: 102rpx;
    font-size: 20rpx;
    color: #656565;
    align-self: center;
    text-align: right;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    text-align: right;
    align-items: flex-end;
  }
  
  .squad-card-user-avatar {
    flex: 1;
    display: flex;
    margin-left: 15rpx;
    position: relative;
  }
  
  .squad-card-user-avatar-sex {
    position: absolute;
    width: 32rpx;
    height: 32rpx;
    top: 5rpx;
    right: -5rpx;
    z-index: 99;
  }
  
  .squad-card-user-avatar-container {
    width: 86rpx;
    height: 86rpx;
    padding: 3rpx;
    border-radius: 50%;
    border: 3rpx solid #FFFFFF;
  }
  
  .squad-card-user-avatar-container.boy {
    border: 3rpx solid #27568E;
  }
  
  .squad-card-user-avatar-container.girl {
    border: 3rpx solid #A42B2D;
  }
  
  .avatar {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
  }
  
  .squad-card-user-detail {
    margin-left: 25rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
  }
  
  .squad-card-user-nick-name {
    font-size: 30rpx;
    line-height: 1;
    color: #2A2A2A;
    font-weight: 500;
    display: flex;
    align-items: center;
  }
  
  .squad-card-user-messages {
    font-size: 22rpx;
    line-height: 1;
    color: #656565;
    display: flex;
    align-items: center;
  }
  
  .squad-card-user-messages .compressive-strength {
    color: #BE3032;
    font-size: 24rpx;
    margin-left: 5rpx;
  }
  
  .squad-card-user-messages-seperator {
    height: 18rpx;
    margin: 0 12rpx;
  }
  
  .squad-detail-invite.squad-detail-invited {
    background-color: #E59D25;
    color: #FFF;
    border: none;
    border-radius: 0;
    font-size: 34rpx;
    line-height: 48rpx;
  }
  
  .squad-detail-apply.squad-detail-join {
    background-color: #E59D25;
    color: #FFF;
    border: none;
    border-radius: 0;
    font-size: 34rpx;
    line-height: 48rpx;
  }
  
  .squad-detail-apply.squad-wait-pass {
    background-color: #28B27B;
    color: #FFF;
    border: none;
    border-radius: 0;
    font-size: 34rpx;
    line-height: 48rpx;
  }
  
  .squad-detail-apply.squad-detail-quit {
    background-color: #BE3032;
    color: #FFF;
    border: none;
    border-radius: 0;
    font-size: 34rpx;
    line-height: 48rpx;
  }
  
  .squad-detail-invite {
    position: fixed;
    width: 100%;
    height: 102rpx;
    bottom: 0;
    font-size: 36rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .squad-detail-apply.squad-detail-applied {
    background-color: #28B27B;
    color: #FFF;
    border: none;
    border-radius: 0;
    font-size: 34rpx;
    line-height: 48rpx;
  }
  
  .squad-detail-applied image {
    width: 109rpx;
    height: 80rpx;
    position: fixed;
    right: -29rpx;
    bottom: 11rpx;
  }
  
  .squad-detail-apply {
    position: fixed;
    width: 100%;
    height: 102rpx;
    bottom: 0;
    font-size: 36rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #E59D25;
  }
  
  .apply-button-tag {
    font-size: 12px;
    position: fixed;
    width: 223rpx;
    height: 51rpx;
    bottom: 83rpx;
    right: 18rpx;
    border-radius: 5rpx;
    z-index: 1;
  }