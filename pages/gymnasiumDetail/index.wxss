page {
    background: #f8f8f8;
}

.love-box {
    display: block;
    padding: 20rpx 0 15rpx;
    box-sizing: border-box;
    line-height: 26rpx;
}

.love-box image {
    display: inline-block;
    margin-right: 2rpx;
    vertical-align: top;
    width: 26rpx;
    height: 26rpx;
}

.love-box text {
    display: inline-block;
    margin-left: 30rpx;
    vertical-align: middle;
    font-size: 24rpx;
}

.detai-box {
    padding: 0 30rpx;
    box-sizing: border-box;
    background: #fff;
}

.detai-box h1 {
    display: block;
    padding: 30rpx 0 10rpx;
    box-sizing: border-box;
    line-height: 40rpx;
    font-size: 40rpx;
}

.detail-tips {
    padding: 5rpx 0 20rpx;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 24rpx;
    font-size: 24rpx;
    color: #666;
}

.detail-tips text {
    float: right;
    vertical-align: top;
}

.sourceShow {
    height: 226rpx;
    white-space: nowrap;
}

.sourceShow scroll-view {
    height: 226rpx;
}

.sourceShow image {
    width: 278rpx;
    height: 208rpx;
    margin-right: 10rpx;
}

.sourceShow video {
    width: 278rpx;
    height: 208rpx;
    margin-right: 10rpx;
}

.top-box {
    position: relative;
    padding: 20rpx 0;
    box-sizing: border-box;
    border-bottom: 1px solid #e6e6e6;
}

.top-box:last-of-type {
    border-bottom: none;
}

.top-box>view {
    position: relative;
    padding-left: 45rpx;
    box-sizing: border-box;
    line-height: 30rpx;
    font-size: 30rpx;
}

.top-box view image {
    position: absolute;
    top: 0;
    left: 0;
    width: 30rpx;
    height: 30rpx;
}

.top-box view s {
    margin-left: 15rpx;
    padding: 0 15rpx;
    border-left: 1px solid #000;
    box-sizing: border-box;
    font-size: 24rpx;
}

.top-box view span {
    display: inline-block;
    margin-right: 10rpx;
    margin-top: 15rpx;
    padding: 0 10rpx;
    border: 1px solid #e6e6e6;
    box-sizing: border-box;
    vertical-align: top;
    line-height: 30rpx;
    font-size: 20rpx;
    color: #666;
}

.top-box view text {
    display: block;
    margin-top: 15rpx;
    font-size: 24rpx;
    color: #666;
}

.business-box {
    padding-right: 100rpx;
    box-sizing: border-box;
}

.pos-box {
    position: relative;
    padding-right: 30rpx;
    box-sizing: border-box;
}

.pos-box>image {
    position: absolute;
    top: 50%;
    right: 0;
    margin-top: -17rpx;
    width: 14rpx;
    height: 34rpx;
}

.top-box>view.phone-box {
    position: absolute;
    padding-left: 0;
    margin-top: -25rpx;
    top: 50%;
    right: 0;
    border-left: 1px solid #e6e6e6;
    width: 100rpx;
    height: 50rpx;
}

.top-box .phone-box image {
    margin-left: -15rpx;
    left: 50%;
    width: 30rpx;
    height: 100%;
}

.brief {
    margin-top: 20rpx;
    padding: 0 30rpx 30rpx;
    box-sizing: border-box;
    background: #fff;
}

.brief h1 {
    display: block;
    padding: 30rpx 0;
    box-sizing: border-box;
    font-size: 34rpx;
    line-height: 34rpx;
}

.brief view {
    float: right;
    font-size: 24rpx;
    color: #666;
}

.brief image {
    display: inline-block;
    margin-left: 10rpx;
    vertical-align: top;
    width: 14rpx;
    height: 34rpx;
}

.brief text {
    box-sizing: border-box;
    font-size: 26rpx;
    line-height: 1.8;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}

.group-title {
    padding: 20rpx 0;
    box-sizing: border-box;
    border-bottom: 1px solid #e6e6e6;
    line-height: 40rpx;
    font-size: 30rpx;
}

.group-title span {
    margin-right: 20rpx;
    font-size: 20rpx;
    width: 40rpx;
    text-align: center;
    color: #fff;
    background: #fe6500;
}

.group-title view {
    float: right;
    font-size: 22rpx;
    color: #666;
}

.group-title view image {
    margin-left: 20rpx;
    margin-right: 5px;
    width: 16rpx;
    height: 40rpx;
    vertical-align: top;
}

.more-btn {
    line-height: 64rpx;
    text-align: center;
    font-size: 26rpx;
}

.more-btn image {
    vertical-align: top;
    width: 24rpx;
    height: 64rpx;
    margin-left: 12rpx;
}

.big-btn-inner {
    line-height: 60rpx;
    width: 100%;
    font-size: 30rpx;
    text-align: center;
    border-radius: 10rpx;
    color: #fff;
    background: #fe6500;
}

.in-title {
    border-bottom: none;
}

.in-title small {
    margin-left: 2rpx;
    font-size: 24rpx;
    color: #888;
}

.comment-box {
    margin-top: 20rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    background: #fff;
}

.seckill {
    margin: 20rpx 0;
    padding: 0 30rpx;
    background: #fff;
}

.seckillTitle {
    position: relative;
    height: 87rpx;
    line-height: 87rpx;
}

.seckillTitle>text {
    font-size: 32rpx;
    color: #333;
    font-weight: bold;
    padding-right: 16rpx;
}

.seckillTitle>view {
    display: inline;
}

.seckillTitle view text:nth-child(1) {
    font-size: 28rpx;
    color: #434343;
    padding-left: 18rpx;
    border-left: 1rpx solid #dedede;
}

.seckillTitle view text:nth-child(2) {
    font-size: 24rpx;
    color: #fff;
    background: #010101;
    border-radius: 50%;
    margin-left: 12rpx;
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    text-align: center;
    line-height: 48rpx;
}

.seckillTitle view text:nth-child(3) {
    font-weight: bold;
    margin-left: 8rpx;
}

.seckillTitle view text:nth-child(4),.seckillTitle view text:nth-child(6) {
    font-size: 24rpx;
    color: #fff;
    background: #010101;
    border-radius: 50%;
    margin-left: 10rpx;
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    text-align: center;
    line-height: 48rpx;
}

.seckillTitle view text:nth-child(5) {
    font-weight: bold;
    margin-left: 8rpx;
}

.seckillTitle view text:nth-child(7) {
    font-size: 24rpx;
    color: #a0a0a0;
    float: right;
    margin-right: 30rpx;
}

.seckillTitle image {
    width: 14rpx;
    height: 28rpx;
    position: absolute;
    right: 0rpx;
    top: 30rpx;
}

.scrollWrapper {
    overflow: hidden;
    white-space: nowrap;
    width: 100%;
    padding-top: 18rpx;
    border-top: 1rpx solid #ddd;
}

scroll-view {
    height: 262rpx;
}

.inlineDisplay {
    display: inline-block;
    margin-right: 20rpx;
    height: 262rpx;
    position: relative;
}

.inlineDisplay>view {
    height: 262rpx;
}

.scrollWrapper image {
    width: 200rpx;
    height: 182rpx;
}

.seckill .petName {
    font-size: 26rpx;
    position: absolute;
    top: 196rpx;
    white-space: nowrap;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
}

.seckill .price {
    font-size: 23rpx;
    position: absolute;
    top: 230rpx;
}

.seckill .price text:nth-child(1) {
    color: #fe6500;
}

.seckill .price text:nth-child(2) {
    margin-left: 10rpx;
    text-decoration: line-through;
    color: #949494;
}

.two-brief {
    padding: 0 15rpx 30rpx;
}

.two-brief h1 {
    padding: 30rpx 15rpx;
    box-sizing: border-box;
}

.brief .brief-box {
    float: none;
    overflow: hidden;
}

.brief-box view {
    float: left;
    padding: 0 15rpx;
    box-sizing: border-box;
    width: 100%;
    margin: 10rpx 0rpx 10rpx 0rpx;
}

.brief-box .img {
    width: 400rpx;
    height: 400rpx;
}

.brief-box h1 {
    display: block;
    padding: 20rpx 0;
    box-sizing: border-box;
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    line-height: 24rpx;
    font-size: 24rpx;
    color: #333;
}

.brief-box b {
    display: block;
    line-height: 28rpx;
    font-size: 28rpx;
    color: #fe6500;
}

.brief-box small {
    margin-left: 5rpx;
    font-size: 20rpx;
}

.empty {
    color: #666;
    font-size: 24rpx;
}

.phoneCall {
    width: 590rpx;
    height: 390rpx;
    border-radius: 10rpx;
    position: fixed;
    left: 80rpx;
    top: 400rpx;
    background: #fff;
    padding: 44rpx 40rpx 0 40rpx;
    z-index: 1;
}

.indicate {
    text-align: center;
    margin-bottom: 60rpx;
}

.shopInfo view {
    text-align: center;
}

.shopInfo view:nth-child(1) {
    font-size: 38rpx;
    color: #f6500f;
    margin-bottom: 10rpx;
}

.shopInfo view:nth-child(2) {
    font-size: 28rpx;
    color: #7c7c7c;
}

.zhuliren {
    font-size: 18rpx;
    padding: 5rpx 3rpx;
    border: 1rpx solid #fe3b46;
    border-radius: 5rpx;
    margin-left: 10rpx;
}

.number {
    margin-top: 16rpx;
    font-size: 22rpx;
}

.number text:nth-child(2) {
    color: #fe3b46;
}

.phoneNumber {
    margin-top: 38rpx;
    height: 92rpx;
    line-height: 92rpx;
    color: #fff;
    font-size: 0;
    text-align: center;
}

.phoneNumber image {
    width: 34rpx;
    height: 34rpx;
    margin-right: 12rpx;
    vertical-align: middle;
}

.phoneNumber text {
    display: inline-block;
    line-height: 92rpx;
    vertical-align: middle;
    font-size: 34rpx;
}

.close {
    width: 23rpx;
    height: 23rpx;
    position: absolute;
    right: 18rpx;
    top: 16rpx;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    position: fixed;
    left: 0;
    top: 0;
}

.img {
    width: 400rpx;
    height: 400rpx;
}