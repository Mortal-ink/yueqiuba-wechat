
.UCenter-bg {
    background-size: cover;
    height: 400rpx;
    display: flex;
    justify-content: center;
    padding-top: 40rpx;
    overflow: hidden;
    position: relative;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0,0,0,0.3);
}

.fontcolorbalck {
    color: black;
}

.list {
    overflow: hidden;
    margin-right: 30rpx;
    margin-left: 30rpx;
    border-radius: 20rpx;
}

.vip {
    position: fixed;
    right: 0rpx;
    top: 50rpx;
    width: 200rpx;
    height: 90rpx;
    background: rgb(255,222,0);
    border-top-left-radius: 45rpx;
    border-bottom-left-radius: 45rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    z-index: 99;
}

.vip .img {
    width: 60rpx;
    height: 60rpx;
    background: #fff;
    border-radius: 50%;
    margin-right: 15rpx;
}

.vip .word {
    display: flex;
    flex-direction: column;
    color: #000;
    font-size: 25rpx;
}

.vip .word .line1 {
    font-weight: bold;
    display: inline-block;
    font-size: 25rpx;
}

.vip .word .line2 {
    display: inline-block;
    color: #444;
    font-size: 21rpx;
}

.png {
    border-radius: 10rpx;
    width: 114rpx;
    height: 114rpx;
}

.UCenter-bg text {
    opacity: 0.8;
}

.UCenter-bg image {
    width: 200rpx;
    height: 200rpx;
}

.UCenter-bg .gif-wave {
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;
    height: 100rpx;
}

map,.mapBox {
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;
    height: 100rpx;
}

map,.mapBox {
    width: 750rpx;
    height: 300rpx;
}