.remind-box {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 300rpx;
}

.font-l2-color {
    color: #b0b5bc;
}

.font-size-30 {
    font-size: 30rpx;
}

.padding-l-20 {
    padding-left: 20rpx;
}

.container {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
}

.remind-img {
    width: 250rpx;
    height: 250rpx;
    padding-bottom: 25rpx;
}

.notHaveList-img {
    width: 250rpx;
    height: 250rpx;
    margin-top: 200rpx;
    background-color: #f9f9f9;
    border-radius: 200rpx;
    box-shadow: 0 0 8px rgba(0,0,0,0.14),0 8px 16px rgba(0,0,0,0.28);
}

.groom {
    position: fixed;
    width: 100%;
    border-bottom: 1px solid #e6e6e6;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #666;
    background: #fff;
    z-index: 9;
}

.groom view {
    float: left;
    width: 33.33%;
    text-align: center;
}

.list-scroll {
    padding-top: 80rpx;
    box-sizing: border-box;
}

.fixed {
    position: fixed;
    left: 0;
    top: 0;
}

.list-layer-box {
    position: absolute;
    left: 0;
    top: 82rpx;
    bottom: 0;
    width: 100%;
    background: rgba(0,0,0,0.38);
    z-index: 9;
}

.scrollLeft {
    width: 100%;
    min-height: 300rpx;
    background: #fff;
    border-right: 1rpx solid #e6e6e6;
    border-left: 1rpx solid #e6e6e6;
    float: left;
}

.scrollLeft view {
    height: 86rpx;
    line-height: 86rpx;
    border-bottom: 1rpx solid rgb(223,223,223);
    padding-left: 30rpx;
    font-size: 24rpx;
}

.groom view>image {
    display: inline-block;
    margin-left: 10rpx;
    vertical-align: top;
    width: 20rpx;
    height: 80rpx;
}

page {
    margin: 0;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 1.42;
    color: #4a4a4a;
    text-align: left;
}

.button-icon {
    width: 64rpx;
    height: 84rpx;
    margin-right: 10rpx;
}

.left-button {
    padding: 30rpx 5rpx 30rpx 30rpx;
    margin: 30rpx 6rpx 30rpx 20rpx;
    border-radius: 8rpx;
}

.right-button {
    padding: 30rpx 5rpx 30rpx 30rpx;
    margin: 30rpx 20rpx 30rpx 6rpx;
    border-radius: 8rpx;
}

.font-title {
    color: #787c87;
    font-size: 32rpx;
}

.font-cont {
    color: #ccc;
    font-size: 26rpx;
}

.bor-b {
    border-bottom: 1px solid #ddd;
}

.bg-light {
    background-color: #f9f9f9;
}

.submit {
    border: 1px solid #d81e06;
    background-color: #d81e06!important;
    font-size: 26rpx;
    padding: 8rpx 40rpx!important;
    border-radius: 50rpx;
    color: #fff;
    line-height: 1.42;
}

.label {
    font-size: 26rpx;
    color: #d81e06;
    border: 1px solid #d81e06;
    border-radius: 8rpx;
    padding: 4rpx 10rpx;
}

.ltfont {
    font-family: "Georgia"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
