.titleImage {
    width: 100%;
}

.cu-form-group .title {
    min-width: calc(4em + 30rpx);
}

.notHaveList-img {
    width: 250rpx;
    margin-top: 50rpx;
    margin-bottom: 50rpx;
    height: 250rpx;
    border-radius: 200rpx;
    background: #fff;
    box-shadow: 0 0 8px rgba(0,0,0,0.14),0 8px 16px rgba(0,0,0,0.28);
}

.OuterFrame {
    width: 94%;
    height: 85%;
    margin: 0 3%;
    margin-top: 20px;
    margin-bottom: 20px;
    border-radius: 15rpx;
    box-shadow: 0 0 4px rgba(0,0,0,0.14),0 4px 8px rgba(0,0,0,0.28);
    overflow: scroll;
    background-color: #ecedf3;
}

.remind-box {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.font-l2-color {
    color: #fff;
}

.margin-top-300 {
    margin-top: 300rpx;
}

.font-size-30 {
    font-size: 30rpx;
}

.padding-l-20 {
    padding-left: 20rpx;
}

.remind-img {
    width: 250rpx;
    height: 250rpx;
    padding-bottom: 25rpx;
}

.swiper {
    height: 400rpx;
    width: 100%;
}

.swiper image {
    height: 100%;
    width: 100%;
}

page {
    margin: 0;
    font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol";
    font-size: 24rpx;
    font-weight: 400;
    line-height: 1.42;
    color: #4a4a4a;
    text-align: left;
}

.container {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
}

.hide {
    display: none;
}

.show {
    display: block;
}